class Solution:
    def arrayPairSum(self, nums: List[int]) -> int:
        
        nums.sort()
        # i = 0
        # res = 0
        # while i < len(nums)-1:
        #     m = min(nums[i],nums[i+1])
        #     res+=m
        #     i+=2
        res = 0
        for i in range(0,len(nums),2):
            res+=nums[i]
        return res
            
       
