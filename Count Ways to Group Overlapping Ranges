class Solution:
    def countWays(self, ranges: List[List[int]]) -> int:
        mod = 10**9+7
        nums = sorted(ranges,key = lambda x : x[0])
        
        groups = 0
        maxi = -1

        for start,end in nums:
            if start > maxi:
                groups+=1
            maxi = max(maxi,end)
        
        return pow(2,groups,mod)
        

        
