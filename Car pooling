class Solution:
    def carPooling(self, trips: List[List[int]], capacity: int) -> bool:
        events = []

        for passengers, start, end in trips:
            events.append((start, passengers))   
            events.append((end, -passengers))
        
        events.sort()
        cap = 0
        for _,val in events:
            cap+=val
            if cap>capacity:
                return False
        return True
    
        
        
        
